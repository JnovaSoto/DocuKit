{
  "name": "docukit",
  "version": "1.0.0",
  "description": "DocuKit is a comprehensive, feature-rich web application designed to serve as the definitive interactive reference guide for HTML tags and their associated metadata. Built on a secure and modern stack, it provides detailed explanations, attribute data, usage examples, and powerful user-centric features, making it an indispensable tool for developers and learners alike.",
  "main": "dist/app.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "start": "node dist/app.js",
    "dev": "node --loader ts-node/esm src/app.ts",
    "build": "tsc",
    "create-db": "node src/db/seed.js",
    "docs": "jsdoc -c jsdoc.json",
    "studio": "npx prisma studio"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JnovaSoto/DocuKit.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/JnovaSoto/DocuKit/issues"
  },
  "homepage": "https://github.com/JnovaSoto/DocuKit#readme",
  "dependencies": {
    "@prisma/client": "^6.19.1",
    "bcrypt": "^6.0.0",
    "connect-sqlite3": "^0.9.16",
    "csurf": "^1.11.0",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-ejs-layouts": "^2.5.1",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "validator": "^13.15.23"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.6",
    "@types/express-ejs-layouts": "^2.5.4",
    "@types/express-session": "^1.18.2",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/validator": "^13.15.10",
    "jest": "^30.2.0",
    "jsdoc": "^4.0.5",
    "prisma": "^6.19.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
